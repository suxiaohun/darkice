<body>
<div class="layout">
  <style>.sojson:before, .sojson:after {
    position: absolute;
    left: 0;
    z-index: 0;
    width: 100%;
    line-height: 30px;
    margin-top: -15px;
    color: #dad8d8;
    font-size: 16px;
    font-weight: 300;
    white-space: nowrap;
  }

  .sojson-foldmarker {
    color: blue;
    text-shadow: #b9f 1px 1px 2px, #b9f -1px -1px 2px, #b9f 1px -1px 2px, #b9f -1px 1px 2px;
    font-family: arial;
    line-height: .3;
    cursor: pointer;
  }

  .sojson-foldgutter {
    width: .7em;
  }

  .sojson-foldgutter-open, .sojson-foldgutter-folded {
    cursor: pointer;
  }

  .sojson-foldgutter-open:after {
    content: "\25BE";
  }

  .sojson-foldgutter-folded:after {
    content: "\25B8";
  }

  .sojson-dialog {
    position: absolute;
    left: 0;
    right: 0;
    background: inherit;
    z-index: 15;
    padding: .1em .8em;
    overflow: hidden;
    color: inherit;
  }

  .sojson-dialog-top {
    border-bottom: 1px solid #eee;
    top: 0;
  }

  .sojson-dialog-bottom {
    border-top: 1px solid #eee;
    bottom: 0;
  }

  .sojson-dialog input {
    border: none;
    outline: none;
    background: transparent;
    width: 20em;
    color: inherit;
    font-family: monospace;
  }

  .sojson-dialog button {
    font-size: 70%;
  }

  .sojson-search-match {
    background: gold;
    border-top: 1px solid orange;
    border-bottom: 1px solid orange;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    opacity: .5;
  }

  .sojson {
    font-family: monospace;
    height: 300px;
    color: black;
  }

  .sojson-lines {
    padding: 4px 0;
  }

  .sojson pre {
    padding: 0 4px;
  }

  .sojson-scrollbar-filler, .sojson-gutter-filler {
    background-color: white;
  }

  .sojson-gutters {
    border-right: 1px solid #ddd;
    background-color: #f7f7f7;
    white-space: nowrap;
  }

  .sojson-linenumber {
    padding: 0 3px 0 5px;
    min-width: 20px;
    text-align: right;
    color: #999;
    white-space: nowrap;
  }

  .sojson-guttermarker {
    color: black;
  }

  .sojson-guttermarker-subtle {
    color: #999;
  }

  .sojson-cursor {
    border-left: 1px solid black;
    border-right: none;
    width: 0;
  }

  .sojson div.sojson-secondarycursor {
    border-left: 1px solid silver;
  }

  .cm-fat-cursor .sojson-cursor {
    width: auto;
    border: 0 !important;
    background: #7e7;
  }

  .cm-fat-cursor div.sojson-cursors {
    z-index: 1;
  }

  .cm-animate-fat-cursor {
    width: auto;
    border: 0;
    -webkit-animation: blink 1.06s steps(1) infinite;
    -moz-animation: blink 1.06s steps(1) infinite;
    animation: blink 1.06s steps(1) infinite;
    background-color: #7e7;
  }

  @-moz-keyframes blink {
    0% {
    }
    50% {
      background-color: transparent;
    }
    100% {
    }
  }

  @-webkit-keyframes blink {
    0% {
    }
    50% {
      background-color: transparent;
    }
    100% {
    }
  }

  @keyframes blink {
    0% {
    }
    50% {
      background-color: transparent;
    }
    100% {
    }
  }

  .cm-tab {
    display: inline-block;
    text-decoration: inherit;
  }

  .sojson-rulers {
    position: absolute;
    left: 0;
    right: 0;
    top: -50px;
    bottom: -20px;
    overflow: hidden;
  }

  .sojson-ruler {
    border-left: 1px solid #ccc;
    top: 0;
    bottom: 0;
    position: absolute;
  }

  .cm-s-default .cm-header {
    color: blue;
  }

  .cm-s-default .cm-quote {
    color: #090;
  }

  .cm-negative {
    color: #d44;
  }

  .cm-positive {
    color: #292;
  }

  .cm-header, .cm-strong {
    font-weight: bold;
  }

  .cm-em {
    font-style: italic;
  }

  .cm-link {
    text-decoration: underline;
  }

  .cm-strikethrough {
    text-decoration: line-through;
  }

  .cm-s-default .cm-keyword {
    color: #708;
  }

  .cm-s-default .cm-atom {
    color: #CC7832;
  }

  .cm-s-default .cm-number {
    color: #4c9cda;
  }

  .cm-s-default .cm-def {
    color: #00f;
  }

  .cm-s-default .cm-variable, .cm-s-default .cm-punctuation, .cm-s-default .cm-property, .cm-s-default .cm-operator {
  }

  .cm-s-default .cm-variable-2 {
    color: #05a;
  }

  .cm-s-default .cm-variable-3 {
    color: #085;
  }

  .cm-s-default .cm-comment {
    color: #a50;
  }

  .cm-s-default .cm-string {
    color: #945DAA;
  }

  .cm-s-default .cm-string-2 {
    color: #f50;
  }

  .cm-s-default .cm-meta {
    color: #555;
  }

  .cm-s-default .cm-qualifier {
    color: #555;
  }

  .cm-s-default .cm-builtin {
    color: #30a;
  }

  .cm-s-default .cm-bracket {
    color: #997;
  }

  .cm-s-default .cm-tag {
    color: #170;
  }

  .cm-s-default .cm-attribute {
    color: #00c;
  }

  .cm-s-default .cm-hr {
    color: #999;
  }

  .cm-s-default .cm-link {
    color: #00c;
  }

  .cm-s-default .cm-error {
    color: #f00;
  }

  .cm-invalidchar {
    color: #f00;
  }

  .sojson-composing {
    border-bottom: 2px solid;
  }

  div.sojson span.sojson-matchingbracket {
    color: #0f0;
  }

  div.sojson span.sojson-nonmatchingbracket {
    color: #f22;
  }

  .sojson-matchingtag {
    background: rgba(255, 150, 0, .3);
  }

  .sojson-activeline-background {
    background: #e8f2ff;
  }

  .sojson {
    position: relative;
    overflow: hidden;
    background: white;
  }

  .sojson-scroll {
    overflow: scroll !important;
    margin-bottom: -30px;
    margin-right: -30px;
    padding-bottom: 30px;
    height: 100%;
    outline: none;
    position: relative;
  }

  .sojson-sizer {
    position: relative;
    border-right: 30px solid transparent;
  }

  .sojson-vscrollbar, .sojson-hscrollbar, .sojson-scrollbar-filler, .sojson-gutter-filler {
    position: absolute;
    z-index: 6;
    display: none;
  }

  .sojson-vscrollbar {
    right: 0;
    top: 0;
    overflow-x: hidden;
    overflow-y: scroll;
  }

  .sojson-hscrollbar {
    bottom: 0;
    left: 0;
    overflow-y: hidden;
    overflow-x: scroll;
  }

  .sojson-scrollbar-filler {
    right: 0;
    bottom: 0;
  }

  .sojson-gutter-filler {
    left: 0;
    bottom: 0;
  }

  .sojson-gutters {
    position: absolute;
    left: 0;
    top: 0;
    min-height: 100%;
    z-index: 3;
  }

  .sojson-gutter {
    white-space: normal;
    height: 100%;
    display: inline-block;
    vertical-align: top;
    margin-bottom: -30px;
  }

  .sojson-gutter-wrapper {
    position: absolute;
    z-index: 4;
    background: none !important;
    border: none !important;
  }

  .sojson-gutter-background {
    position: absolute;
    top: 0;
    bottom: 0;
    z-index: 4;
  }

  .sojson-gutter-elt {
    position: absolute;
    cursor: default;
    z-index: 4;
  }

  .sojson-gutter-wrapper::selection {
    background-color: transparent
  }

  .sojson-gutter-wrapper::-moz-selection {
    background-color: transparent
  }

  .sojson-lines {
    cursor: text;
    min-height: 1px;
  }

  .sojson pre {
    -moz-border-radius: 0;
    -webkit-border-radius: 0;
    border-radius: 0;
    border-width: 0;
    background: transparent;
    font-family: inherit;
    font-size: inherit;
    margin: 0;
    white-space: pre;
    word-wrap: normal;
    line-height: inherit;
    color: inherit;
    z-index: 2;
    position: relative;
    overflow: visible;
    -webkit-tap-highlight-color: transparent;
    -webkit-font-variant-ligatures: contextual;
    font-variant-ligatures: contextual;
  }

  .sojson-wrap pre {
    word-wrap: break-word;
    white-space: pre-wrap;
    word-break: normal;
  }

  .sojson-linebackground {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 0;
  }

  .sojson-linewidget {
    position: relative;
    z-index: 2;
    overflow: auto;
  }

  .sojson-rtl pre {
    direction: rtl;
  }

  .sojson-code {
    outline: none;
  }

  .sojson-scroll, .sojson-sizer, .sojson-gutter, .sojson-gutters, .sojson-linenumber {
    -moz-box-sizing: content-box;
    box-sizing: content-box;
  }

  .sojson-measure {
    position: absolute;
    width: 100%;
    height: 0;
    overflow: hidden;
    visibility: hidden;
  }

  .sojson-cursor {
    position: absolute;
    pointer-events: none;
  }

  .sojson-measure pre {
    position: static;
  }

  div.sojson-cursors {
    visibility: hidden;
    position: relative;
    z-index: 3;
  }

  div.sojson-dragcursors {
    visibility: visible;
  }

  .sojson-focused div.sojson-cursors {
    visibility: visible;
  }

  .sojson-selected {
    background: #d9d9d9;
  }

  .sojson-focused .sojson-selected {
    background: #d7d4f0;
  }

  .sojson-crosshair {
    cursor: crosshair;
  }

  .sojson-line::selection, .sojson-line > span::selection, .sojson-line > span > span::selection {
    background: #d7d4f0;
  }

  .sojson-line::-moz-selection, .sojson-line > span::-moz-selection, .sojson-line > span > span::-moz-selection {
    background: #d7d4f0;
  }

  .cm-searching {
    background: #ffa;
    background: rgba(255, 255, 0, .4);
  }

  .cm-force-border {
    padding-right: .1px;
  }

  @media print {
    .sojson div.sojson-cursors {
      visibility: hidden;
    }
  }

  .cm-tab-wrap-hack:after {
    content: '';
  }

  span.sojson-selectedtext {
    background: none;
  }

  pre, code, tt {
    font: 0.95em 'andale mono', monotype, 'lucida console', monospace;
    line-height: 1.5;
  }

  .seventy {
    float: left;
    width: 728px;
  }

  .sixty {
    float: left;
    width: 60%;
  }

  .fifty {
    float: left;
    width: 50%;
  }

  .fifty.pad {
    padding: 0 10px;
  }

  .memberful-button {
    float: right;
    margin-top: 17px;
  }

  .memberful-button a {
    background: #1E8BC3;
    padding: 10px 15px 11px;
    color: #fff;
    border-radius: 1px;
    margin: 0;
    text-decoration: none;
    font-size: 14px;
    font-weight: 600;
  }

  .memberful-button a:hover {
    background: #555;
  }

  .info {
    display: block;
    padding: 0 0 30px;
    clear: both;
    width: 100%;
    border-bottom: 1px solid #eee;
  }

  p {
    font-size: 14px;
  }

  p.heading {
    margin-top: 20px;
    font-size: 15px;
    font-weight: 700;
    padding-bottom: 0;
  }

  p code, li code {
    line-height: 2.1
  }

  .hide {
    display: none;
  }

  main {
    display: block;
    width: 100%;
    max-width: 950px;
    margin: 0 auto;
    padding: 1.5em 0;
  }

  .sojson-dialog input {
    vertical-align: middle;
  }

  #code, .sojson {
    width: 100%;
    height: 700px;
    border: 1px solid #e1e1e1;
    font-size: 15px;
  }

  .line-error {
    background-color: #FBE3E4;
  }

  #result-container {
    display: none;
    padding-top: 15px;
  }

  #result-container.shown {
    display: block;
  }

  #result-container h3 {
    font-size: 1.3em;
  }

  #result {
    border: 1px solid transparent;
    padding: 0.8em;
  }

  #result:empty {
    display: none;
  }

  #results.error {
    background: #FBE3E4;
    color: #D12F19;
    border-color: #FBC2C4;
  }

  #results.success {
    background: #E6EFC2;
    color: #529214;
    border-color: #C6D880;
  }

  div.success {
    border-color: #C6D880;
  }

  div.error {
    border-color: #FBC2C4;
  }</style>
  <script>
      function test2() {
          console.log($("#articlesS"))
      }
  </script>
  <%#= javascript_pack_tag  'custom' %>

  <div id="so_box" class="layui-row layout-container  is-active">
    <div class="layout-main-view">
      <div style="padding-bottom: 10px"></div>
      <div class="layui-anim layui-anim-scale" style="width: 100%;padding-right: 30px;">
        <form id="JSONValidate" method="post" action="." name="main" onsubmit="return false" autocomplete="off">
          <textarea id="code" name="code" class="json_input" style="display: none;resize:both">

          </textarea>

          <div class="grippie"></div>
          <div class="panel panel-default" style="margin-top: 10px;display:none">
            <div class="panel-body">
              <ol id="articlesS"></ol>
            </div>
          </div>
          <div class="layui-btn-group pt10 pb20">
            <button class="layui-btn " name="validate" data-ga="validate" type="submit" id="validate1">test</button>
            <button class="layui-btn " name="validate" data-ga="validate" type="submit" id="validate">校验 / 格式化</button>
            <!--            <a class="layui-btn layui-btn-primary" execute-data="1" href="javascript:void(0);">压缩</a>-->
            <!--            <a class="layui-btn layui-btn-primary" execute-data="2" href="javascript:void(0);">转义</a>-->
            <!--            <a class="layui-btn layui-btn-primary" execute-data="4" href="javascript:void(0);">去转义</a>-->
            <!--            <a class="layui-btn layui-btn-primary" execute-data="5" href="javascript:void(0);">Unicode转中文</a>-->
            <!--            <a class="layui-btn layui-btn-primary" execute-data="6" href="javascript:void(0);">中文转Unicode</a>-->
            <a class="layui-btn layui-btn-primary copy" click-type="copycode" href="javascript:void(0);" data-clipboard-target="#code">复制结果</a>
            <a class="layui-btn layui-btn-primary" id="clear" data-target=".json_input" href="javascript:void(0);"><i class="layui-icon"></i>清空</a>
            <a class="layui-btn layui-btn-primary" id="saveFile" save-target=".json_input" save-type=".json" href="javascript:void(0);">保存本地</a>
          </div>
          <pre class="success " id="results"></pre>
        </form>
      </div>
    </div>
  </div>

</div>

</body>





